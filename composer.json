{
  "name": "dcarbone/php-object-merge",
  "type": "library",
  "description": "Object property merging in PHP",
  "license": "Apache-2.0",
  "homepage": "https://github.com/dcarbone/php-object-merge",
  "authors": [
    {
      "name": "Daniel Carbone",
      "email": "daniel.p.carbone@gmail.com"
    }
  ],
  "keywords": [
    "php",
    "object-merge",
    "merge"
  ],
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "ext-json",
    "phpunit/phpunit": "^9.1"
  },
  "autoload": {
    "files": [
      "files/constants.php",
      "files/functions.php"
