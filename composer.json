    "psr-4": {
      "DCarbone\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "files/constants.php",
      "files/functions.php"
    ],
    "psr-4": {
      "DCarbone\\": "src/",
      "DCarbone\\Tests\\": "tests/"
    }
  }
}